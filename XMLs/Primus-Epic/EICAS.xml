<!-- Warnings and Indicators -->

<animation>
    <type>select</type>
	<object-name>parking.brake</object-name>
  	<condition>
    	<equals>
    	<property>/controls/gear/brake-parking</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>e1.failure</object-name>
  	<condition>
    	<equals>
    	<property>/engines/engine/running</property>
    	<value>0</value>
    	</equals>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>e2.failure</object-name>
  	<condition>
    	<equals>
    	<property>/engines/engine[1]/running</property>
    	<value>0</value>
    	</equals>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>critical.fuel</object-name>
  	<condition>
    	<less-than>
    	<property>/consumables/fuel/total-fuel-lbs</property>
    	<value>2000</value>
    	</less-than>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>spoilers.armed</object-name>
  	<condition>
	<not>
    	<equals>
    	<property>/surface-positions/speedbrake-pos-norm</property>
    	<value>0</value>
    	</equals>
	</not>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>stall.warning</object-name>
  	<condition>
	<and>
    	<less-than>
    	<property>/velocities/airspeed-kt</property>
    	<value>126</value>
    	</less-than>
    	<greater-than>
    	<property>/position/altitude-agl-ft</property>
    	<value>200</value>
    	</greater-than>
	</and>
  	</condition>
</animation>
<animation>
    <type>select</type>
	<object-name>mach.trim</object-name>
  	<condition>
	<and>
    	<equals>
    	<property>/surface-positions/flap-pos-norm</property>
    	<value>0</value>
    	</equals>
    	<less-than>
    	<property>/velocities/airspeed-kt</property>
    	<value>161</value>
    	</less-than>
    	<greater-than>
    	<property>/position/altitude-agl-ft</property>
    	<value>200</value>
    	</greater-than>
	</and>
  	</condition>
</animation>

<!-- APU Indicators -->

