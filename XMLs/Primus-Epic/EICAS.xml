<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>
  
  <path>../../Models/Primus-Epic/primus_epic_eicas.ac</path>

	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>

<!-- Gears and Needles -->

<animation>
    <type>select</type>
	<object-name>gear0.down</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[0]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<animation>
  	<type>select</type>
  	<object-name>gear1.down</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[1]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<animation>
  	<type>select</type>
  	<object-name>gear2.down</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[2]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

<animation>
    <type>select</type>
	<object-name>gear0.up</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[0]/position-norm</property>
    	<value>0</value>
    	</equals>
  	</condition>
</animation>

<animation>
  	<type>select</type>
  	<object-name>gear1.up</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[1]/position-norm</property>
    	<value>0</value>
    	</equals>
  	</condition>
</animation>

<animation>
  	<type>select</type>
  	<object-name>gear2.up</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[2]/position-norm</property>
    	<value>0</value>
    	</equals>
  	</condition>
</animation>

<animation>
	<type>rotate</type>
   	<object-name>Flapneedle</object-name>
   	<property>surface-positions/flap-pos-norm</property>
   	<interpolation>
        <entry>
       		<ind>0.0</ind><dep>0.0</dep>
     	</entry>
     	<entry>
       		<ind>1.0</ind><dep>30.0</dep>
     	</entry>
   	</interpolation>
   	<center>
    	<x-m>0</x-m>
     	<y-m>-0.0421</y-m>
     	<z-m>-0.0994</z-m>
   	</center>
   	<axis>
     	<x>-1.0</x>
     	<y>0</y>
     	<z>0.0</z>
   	</axis>
</animation>

<animation>
	<type>rotate</type>
   	<object-name>roll.trim</object-name>
   	<property>controls/flight/aileron-trim</property>
	<factor>20</factor>
	<center>
	<x-m>0</x-m>
	<y-m>0.0325</y-m>
	<z-m>-0.1041</z-m>
	</center>
   	<axis>
     	<x>-1</x>
     	<y>0</y>
     	<z>0</z>
   	</axis>
</animation>

<animation>
	<type>translate</type>
   	<object-name>pitch.trim</object-name>
   	<property>controls/flight/elevator-trim</property>
	<factor>0.012</factor>>
   	<axis>
     	<x>0</x>
     	<y>0</y>
     	<z>1</z>
   	</axis>
</animation>

<animation>
	<type>translate</type>
   	<object-name>yaw.trim</object-name>
   	<property>controls/flight/rudder-trim</property>
	<factor>0.012</factor>>
   	<axis>
     	<x>0</x>
     	<y>1</y>
     	<z>0</z>
   	</axis>
</animation>

