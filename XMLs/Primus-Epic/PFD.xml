<!-- Heading Digits Textranlate -->

	<animation>
		<name>Heading setting 100's</name>
		<type>textranslate</type>
		<object-name>Hdg100</object-name>
		<property>controls/autoflight/heading-select</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
