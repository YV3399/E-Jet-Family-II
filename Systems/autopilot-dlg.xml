				</text>
				<group>
					<layout>hbox</layout>
					<row>2</row>
					<col>1</col>
					<button>
						<halign>right</halign>
						<legend>&lt;</legend>
						<binding>
							<command>property-adjust</command>
							<property>controls/autoflight/vertical-speed-select</property>
							<step>-100</step>
							<min>-9000</min>
						</binding>
					</button>
					<input>
						<property>controls/autoflight/vertical-speed-select</property>
						<binding>
							<command>dialog-apply</command>
						</binding>
						<live type="bool">true</live>
					</input>
					<button>
						<halign>left</halign>
						<legend>&gt;</legend>
						<binding>
							<command>property-adjust</command>
							<property>controls/autoflight/vertical-speed-select</property>
							<step>100</step>
							<max>9000</max>
						</binding>
					</button>
				</group>
			</group>

			<group>
				<layout>hbox</layout>
				<group>
					<layout>hbox</layout>
					<text>
						<label>O</label>
						<visible>
							<equals>
								<property>controls/autoflight/vertical-mode</property>
								<value>1</value>
							</equals>
						</visible>
					</text>
					<button>
						<legend>ALT</legend>
						<binding>
							<command>property-cycle</command>
							<property>controls/autoflight/vertical-mode</property>
							<value>1</value>
							<value>0</value>
						</binding>
					</button>
					<text>
						<label>O</label>
						<visible>
							<equals>
								<property>controls/autoflight/vertical-mode</property>
								<value>1</value>
							</equals>
						</visible>
					</text>
				</group>
				<group>
					<layout>hbox</layout>
					<text>
						<label>O</label>
						<visible>
							<equals>
								<property>controls/autoflight/vertical-mode</property>
								<value>2</value>
							</equals>
						</visible>
					</text>
					<button>
						<legend>VS</legend>
						<binding>
							<command>property-cycle</command>
							<property>controls/autoflight/vertical-mode</property>
							<value>2</value>
							<value>0</value>
						</binding>
					</button>
					<text>
						<label>O</label>
						<visible>
							<equals>
								<property>controls/autoflight/vertical-mode</property>
								<value>2</value>
							</equals>
						</visible>
					</text>
				</group>
			</group>
		</group>
	</group>

	<hrule>
		<color>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</color>
	</hrule>

	<group>
		<layout>table</layout>
		<text>
			<row>0</row>
			<col>0</col>
			<halign>right</halign>
			<label>[ IAS ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autothrottle-engaged</property>
					<equals>
						<property>controls/autoflight/speed-mode</property>
						<value>0</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>0</col>
			<halign>right</halign>
			<label>[ MACH ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autothrottle-engaged</property>
					<equals>
						<property>controls/autoflight/speed-mode</property>
						<value>1</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>1</col>
			<halign>right</halign>
			<label>||</label>
		</text>
		<text>
			<row>0</row>
			<col>2</col>
			<label>[ HOLD ROLL ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>controls/autoflight/basic-roll-mode</property>
						<value>0</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>2</col>
			<label>[ HOLD HDG ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>controls/autoflight/basic-roll-mode</property>
						<value>1</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>2</col>
			<label>[ HDG ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>1</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>2</col>
			<label>[ NAV ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>2</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>2</col>
			<label>[ APPR ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>3</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>3</col>
			<halign>left</halign>
			<label>||</label>
		</text>
		<text>
			<row>0</row>
			<col>4</col>
			<halign>left</halign>
			<label>[ PITCH ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/vertical-mode</property>
						<value>0</value>
					</equals>
					<not>
						<equals>
							<property>controls/autoflight/lateral-mode</property>
							<value>3</value>
						</equals>
					</not>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>4</col>
			<halign>left</halign>
			<label>[ G/S ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/vertical-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>controls/autoflight/lateral-mode</property>
						<value>3</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>4</col>
			<halign>left</halign>
			<label>[ ALT ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/vertical-mode</property>
						<value>1</value>
					</equals>
				</and>
			</visible>
		</text>
		<text>
			<row>0</row>
			<col>4</col>
			<halign>left</halign>
			<label>[ VS ]</label>
			<visible>
				<and>
					<property>autopilot/internal/autoflight-engaged</property>
					<equals>
						<property>controls/autoflight/vertical-mode</property>
						<value>2</value>
					</equals>
				</and>
			</visible>
		</text>
	</group>

	<hrule>
		<color>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</color>
	</hrule>

	<button>
		<legend>Close</legend>
		<default type="bool">true</default>
		<pref-width>180</pref-width>
		<key>Esc</key>
		<binding>
			<command>dialog-close</command>
		</binding>
	</button>

</PropertyList>
