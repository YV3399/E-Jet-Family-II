        <volume>
            <property>sim/sound/effects/volume</property>
            <factor>2.0</factor>
        </volume>
        <position>
            <x>-24</x>
            <y>0</y>
            <z>0.18</z>
        </position>
        <reference-dist>0.5</reference-dist>
        <max-dist>1.0</max-dist>
    </speedbrake>

    <engines>
        <name>left reverse-thrust</name>
        <mode>looped</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/reverser.wav</path>
