        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>100.0</max-dist>
    </nosmoke>

    <!-- Overspeed warning -->
    <overspeed>
        <name>Overspeed warning</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/Ovspd.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>velocities/airspeed-kt</property>
                <value>563</value>
            </greater-than>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </overspeed>

