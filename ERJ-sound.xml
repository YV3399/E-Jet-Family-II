<?xml version="1.0"?>

<PropertyList>

<fx>
    <engine>
        <name>left engine</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[0]/cutoff</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0.0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
		<name>low l out</name>
		<mode>looped</mode>
		<path>Aircraft/E-jet-family/Sounds/SoundPack/xlow.wav</path>
		<property>engines/engine[0]/n1</property>
		<condition>
			<not><property>sim/current-view/internal</property></not>
		</condition>
		<volume>
			<property>engines/engine[0]/n1</property>
			<factor>0.044</factor>
			<max>1</max>
		</volume>
		<pitch>
			<property>engines/engine[0]/n1</property>
			<offset>0</offset>
			<factor>0.02</factor>
			<max>2</max>
		</pitch>
	</engine>

    <engine>
        <name>left n1_2</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/turbine.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/n1</property>
            <factor>0.1</factor>
            <min>0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>
    
    <engine>
        <name>left reverse thrust</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/reverser.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>2</value>
                </greater-than>
                <greater-than>
                    <property>engines/engine[0]/reverser-pos-norm</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
        <volume>
            <property>engines/engine[0]/n1</property>
            <min>0.0</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>left engine inside</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/inside.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>2</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
        <name>right engine</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[0]/cutoff</property>
