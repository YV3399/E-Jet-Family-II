        <max-dist>500.0</max-dist>
    </engine>
    <engine>
		<name>low r out</name>
		<mode>looped</mode>
		<path>Aircraft/E-jet-family/Sounds/SoundPack/xlow.wav</path>
		<property>engines/engine[1]/n1</property>
		<condition>
			<not><property>sim/current-view/internal</property></not>
		</condition>
		<volume>
			<property>engines/engine[1]/n1</property>
			<factor>0.044</factor>
			<max>1</max>
		</volume>
		<pitch>
			<property>engines/engine[1]/n1</property>
			<offset>0</offset>
			<factor>0.02</factor>
			<max>2</max>
		</pitch>
	</engine>

    <engine>
        <name>right n1_2</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/turbine.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[1]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[1]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[1]/n1</property>
            <factor>0.1</factor>
            <min>0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>right reverse thrust</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/reverser.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[1]/n1</property>
                    <value>2</value>
                </greater-than>
                <greater-than>
                    <property>engines/engine[1]/reverser-pos-norm</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
        <volume>
            <property>engines/engine[1]/n1</property>
            <min>0.0</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>right engine inside</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/inside.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[1]/n1</property>
                    <value>2</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <pitch>
            <property>/engines/engine[1]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[1]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <gear1>
        <name>gear</name>
        <mode>in-transit</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/gear.wav</path>
        <property>/gear/gear[0]/position-norm</property>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <pitch>
            <internal>dt_play</internal>
            <offset>0.7</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear1>

    <gear1>
        <name>gear-lock</name>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <equals>
                <property>/gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
<position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>20.0</max-dist>
    </gear1>

     <rumblefront>
        <name>nose wheel rumble</name>
        <mode>looped</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/rumble.wav</path>
        <condition>
            <property>/gear/gear[0]/wow</property>
        </condition>
        <volume>
            <property>/velocities/groundspeed-kt</property>
            <type>log</type>
            <factor>0.25</factor>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>25.0</max-dist>
    </rumblefront>

    <rumblemain>
        <name>main gear rumble</name>
        <mode>looped</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/rumble.wav</path>
        <condition>
            <or>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>/velocities/groundspeed-kt</property>
            <type>log</type>
            <factor>0.25</factor>
        </volume>
        <position>
            <x>-6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>25.0</max-dist>
    </rumblemain>

<squeal0>
        <name>squeal</name>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/touchdown.wav</path>
        <condition>
            <or>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>sim/sound/Ovolume</property>
        </volume>
        <volume>
            <property>/velocities/speed-down-fps</property>
            <factor>0.05</factor>
        </volume>
        <volume>
            <property>/velocities/airspeed-kt</property>
            <factor>0.01</factor>
        </volume>
        <volume>
            <internal>dt_stop</internal>
            <factor>0.05</factor>
            <max>1.0</max>
        </volume>
        <pitch>
            <property>/velocities/airspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
        <position>
            <x>-6.5</x>
            <y>0</y>
            <z>-5</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>40.0</max-dist>
    </squeal0>

    <squeal1>
      <name>squeal</name>
      <path>Aircraft/E-jet-family/Sounds/SoundPack/Roll.wav</path>
      <condition>
        <property>/gear/gear[0]/wow</property>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal1>

    <flap>
        <name>flaps</name>
        <mode>in-transit</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/flaps.wav</path>
        <property>/surface-positions/flap-pos-norm</property>
        <volume>
            <property>sim/sound/effects/volume</property>
            <max>0.5</max>
        </volume>
        <position>
            <x>-9</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </flap>

    <wind>
        <name>wind</name>
        <mode>looped</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/wind.wav</path>
        <property>/velocities/airspeed-kt</property>
        <volume>
            <property>/position/altitude-ft</property>
            <factor>-0.000015</factor>
            <offset>1.0</offset>
            <min>0.1</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>/velocities/airspeed-kt</property>
            <factor>0.0015</factor>
            <min>0.03</min>
            <max>0.25</max>
        </volume>
        <pitch>
            <property>/velocities/airspeed-kt</property>
            <factor>0.01</factor>
            <offset>1.25</offset>
        </pitch>
        <position>
            <x>-24</x>
            <y>0</y>
            <z>0.86</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </wind>

    <stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>Aircraft/E-jet-family/Sounds/SoundPack/stall.wav</path>
        <condition>
            <property>sim/sound/stall-horn</property>
        </condition>
        <volume>
            <offset>0.5</offset>
        </volume>
        <position>
            <x>-24</x>
            <y>0</y>
            <z>0.18</z>
        </position>
        <reference-dist>0.5</reference-dist>
        <max-dist>1.0</max-dist>
